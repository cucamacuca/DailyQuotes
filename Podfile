source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
use_frameworks!

workspace 'DailyQuotes.xcworkspace'
xcodeproj 'Quotes.xcodeproj'

target :Quotes do
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Alamofire', '~> 1.2'
  pod 'SwiftyJSON', '~> 2.2'
end

target :'Quotes WatchKit Extension' do
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Alamofire', '~> 1.3'
  pod 'SwiftyJSON', '~> 2.2'
end